    "jwt-decode": "^4.0.0",
    "next": "^13.5.4",
    "next-auth": "^4.24.7",
    "next-pwa": "^5.6.0",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-pro-sidebar": "^1.0.0-alpha.7",
    "react-redux": "^9.1.0",
