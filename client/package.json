    "@types/node": "^20.8.4",
    "@types/react": "^18.2.27",
    "@types/react-dom": "^18.2.12",
    "eslint": "^8.51.0",
    "eslint-config-next": "^13.5.4",
    "eslint-config-prettier": "^9.0.0",
